//1
#include <stdio.h>

int ifprime(int num1);
void printprime(int num11);
int ifarm(int num2);
void printarm(int num22);
int ifper(int num3);
void printper(int num33);

int main() 
{
    int a;
    scanf("%d", &a);
    printprime(a);
    printf("\n");
    printarm(a);
    printf("\n");
    printper(a);
    return 0;
}

void printprime(int num11)
{
    if (ifprime(num11))
    {
        printf("%d is a prime number", num11);
    }
    else
    {
        printf("%d is not a prime number", num11);
    }
}
int ifprime(int num1)
{
    int i;
    for (i=2; i<num1; i++)
    {
        if (num1%i==0)
        {
            return 0;
        }
    }
    return 1;
}

void printarm(int num22)
{
    if (ifarm(num22))
    {
        printf("%d is a armstrong number", num22);
    }
    else
    {
        printf("%d is not a armstrong number", num22);
    }
}

int ifarm(int num2)
{
    int r, sum=0, temp;
    for(temp=num2; num2!=0; num2=num2/10)
    {
        r=num2%10;
        sum=sum+(r*r*r);
    }
    if (sum=temp)
    {
        return 0;
    }
    return 1;
}

void printper(int num33)
{
    if (ifper(num33))
    {
        printf("%d is a perfect number", num33);
    }
    else
    {
        printf("%d is not a perfect number", num33);
    }
}

int ifper(int num3)
{
    int i, sum=0;
    for (i=1; i<num3; i++)
    {
        if (num3%i==0)
        {
            sum=sum+1;
        }
    }
    if (num3=sum)
    {
        return 0;
    }
    return 1;
}

//3
#include <stdio.h>


int main() {
    char s[200];
    scanf("%s",s);
    int l=strlen(s);
    int j,i=0,a=l;
    int f=0;
    char a1;
    printf("Length of the string is %d\n",l);
    for(i=0;i<l;i++)
    {
        for(j=i+1;j<l;++j)
        {
            if (s[i] == s[j])
            {
                a=a-1;
            }
            if (s[i] == s[j] && f==0)
            {
                f=1;
                a1=s[i];
            }
        }
    }
    printf("Word frequency is %d\n",a);
    if (f ==0)
    {
        printf("No character is repeated in the string");
    }
    else
    {
        printf("First repeated character is %c\n",a1);
    }
    for(i=0;i<l;i++)
    {
        f=0;
        for(j=i+1;j<l;++j)
        {
            if (s[i] == s[j])
            {
                f=1;
            }
        }
        if (f==0)
        {
            printf("First non-repeated character is %c",s[i]);
            break;
        }
    }
    
    return 0;
}
